{
  "name": "si-units",
  "version": "1.0.0",
  "description": "A comprehensive SI units library with support for time, length, mass, temperature, and more",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./time": {
      "import": "./dist/time.mjs",
      "require": "./dist/time.cjs",
      "types": "./dist/time.d.ts"
    },
    "./length": {
      "import": "./dist/length.mjs",
      "require": "./dist/length.cjs",
      "types": "./dist/length.d.ts"
    },
    "./mass": {
      "import": "./dist/mass.mjs",
      "require": "./dist/mass.cjs",
      "types": "./dist/mass.d.ts"
    },
    "./temperature": {
      "import": "./dist/temperature.mjs",
      "require": "./dist/temperature.cjs",
      "types": "./dist/temperature.d.ts"
    },
    "./volume": {
      "import": "./dist/volume.mjs",
      "require": "./dist/volume.cjs",
      "types": "./dist/volume.d.ts"
    },
    "./area": {
      "import": "./dist/area.mjs",
      "require": "./dist/area.cjs",
      "types": "./dist/area.d.ts"
    },
    "./data": {
      "import": "./dist/data.mjs",
      "require": "./dist/data.cjs",
      "types": "./dist/data.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "si-units",
    "units",
    "conversion",
    "time",
    "length",
    "mass",
    "temperature",
    "volume",
    "area",
    "data"
  ],
  "author": "Your Name",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.0.0",
    "jest": "^29.0.0",
    "rollup": "^3.0.0",
    "rollup-plugin-dts": "^5.0.0",
    "tslib": "^2.6.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}